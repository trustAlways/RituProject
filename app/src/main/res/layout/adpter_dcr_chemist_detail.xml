<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/x_et_bg"
    android:layout_marginTop="@dimen/mrg1"

    >

    <RelativeLayout
        android:id="@+id/rr_dcr_txt_doc_detail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="@dimen/mrg1"
        >

        <RelativeLayout
            android:id="@+id/rr_full_descp_doc"
            android:layout_width="match_parent"
            android:layout_height="50dp">

            <RelativeLayout
                android:id="@+id/rr_doc_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:layout_marginLeft="@dimen/mrg1"
                android:padding="6dp"
                >
                <TextView
                    android:id="@+id/rr_chemist_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/size0"
                    android:text="Dr. anil Kumar"
                    android:visibility="visible"
                    android:singleLine="true"
                    android:inputType="textCapWords|textNoSuggestions"
                    android:capitalize="words"
                    android:textColor="@color/colorPrimary"/>



            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/Arrow_down"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:visibility="visible"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/mrg2"
                android:layout_marginLeft="@dimen/mrg2"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="15dp"
                    android:src="@drawable/arrow_down_icon"
                    android:layout_centerInParent="true"/>



            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Arrow_up"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/mrg2"
                android:layout_marginLeft="@dimen/mrg2"
                android:visibility="gone"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="15dp"
                    android:layout_centerInParent="true"
                    android:src="@drawable/arrow_up_icon" />


            </RelativeLayout>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rr_full_descp_timedate2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:layout_below="@+id/rr_full_descp_doc">

            <RelativeLayout
                android:id="@+id/rr_call_dureation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/mrg1"
                android:visibility="visible"

                >
                <TextView
                    android:id="@+id/set_duration"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:text="Call Duration :"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/mrg4"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/calls_duratn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:layout_toRightOf="@id/set_duration"
                    android:text="5 hour"
                    android:textSize="@dimen/mrg4"
                    android:paddingRight="@dimen/mrg2"
                    android:visibility="visible" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rr_gifts"
                android:layout_width="match_parent"
                android:layout_height="@dimen/size3"
                android:layout_marginTop="@dimen/mrg2"
                android:layout_below="@+id/rr_call_dureation"
                >

                <RelativeLayout
                    android:id="@+id/rr_degree"
                    android:layout_marginTop="@dimen/mrg1"
                    android:layout_width="@dimen/size14"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/mrg2"

                    >
                    <TextView
                        android:id="@+id/txt_degree"
                        android:layout_width="@dimen/size13"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/mrg4"
                        android:textColor="@color/colorPrimary"
                        android:visibility="visible"
                        android:singleLine="true"
                        android:text="Gifts"
                        />



                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rr_testing_count"
                    android:layout_width="@dimen/size19"
                    android:layout_height="@dimen/size3"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    >


                    <RelativeLayout
                        android:id="@+id/rr_left_testing"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/x_et_bg_cmpny_login"
                        android:elevation="@dimen/mrg1"
                        android:layout_toLeftOf="@+id/rr_right_qty"
                        android:layout_marginRight="@dimen/mrg1"

                        >

                        <EditText
                            android:id="@+id/spn_product"
                            android:layout_width="@dimen/size7"
                            android:layout_height="@dimen/size2"
                            android:text="Testing"
                            android:focusable="false"
                            android:singleLine="true"
                            android:paddingLeft="@dimen/size0"
                            android:textColor="@color/colorPrimary"
                            android:background="@android:color/transparent"
                            ></EditText>

                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rr_right_qty"
                        android:layout_width="@dimen/size4"
                        android:layout_height="@dimen/size2"
                        android:layout_alignParentRight="true"
                        android:background="@drawable/x_et_bg_cmpny_login"
                        android:elevation="@dimen/mrg1"
                        android:layout_marginRight="@dimen/mrg1"

                        >

                        <EditText
                            android:id="@+id/edt_spn_product_qty"
                            android:layout_width="@dimen/size4"
                            android:layout_height="@dimen/size2"
                            android:focusable="false"
                            android:singleLine="true"
                            android:text="10"
                            android:layout_centerInParent="true"
                            android:paddingLeft="@dimen/size1"
                            android:textColor="@color/colorPrimary"
                            android:textColorHint="@color/colorPrimary"
                            android:textSize="@dimen/mrg4"
                            android:background="@android:color/transparent"
                            ></EditText>

                    </RelativeLayout>




                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rr_pobamount"
                android:layout_width="match_parent"
                android:layout_height="@dimen/size3"

                android:layout_below="@+id/rr_gifts"
                >

                <RelativeLayout
                    android:id="@+id/rr_pobamount_testing"
                    android:layout_marginTop="@dimen/mrg1"
                    android:layout_width="@dimen/size14"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/mrg2"

                    >
                    <TextView
                        android:id="@+id/txt_pobamount"
                        android:layout_width="@dimen/size13"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/mrg4"
                        android:textColor="@color/colorPrimary"
                        android:visibility="visible"
                        android:singleLine="true"
                        android:text="POB amount"
                        />



                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rr_testing_pobamount"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/size3"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/mrg1"
                    >


                    <RelativeLayout
                        android:id="@+id/rr_left_testing_pobamount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/x_et_bg_cmpny_login"
                        android:elevation="@dimen/mrg1"

                        >

                        <EditText
                            android:id="@+id/spn_product_pobamount"
                            android:layout_width="@dimen/size7"
                            android:layout_height="@dimen/size2"
                            android:hint="Testing"
                            android:focusable="false"
                            android:singleLine="true"
                            android:textColorHint="@color/colorPrimary"
                            android:paddingLeft="@dimen/size0"
                            android:textColor="@color/colorPrimary"
                            android:background="@android:color/transparent"
                            ></EditText>

                    </RelativeLayout>

                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rr_stockiest1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/size3"

                android:layout_below="@+id/rr_pobamount"
                >

                <RelativeLayout
                    android:id="@+id/rr_stockiest1_full"
                    android:layout_marginTop="@dimen/mrg1"
                    android:layout_width="@dimen/size14"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/mrg2"
                    android:layout_centerVertical="true"

                    >
                    <TextView
                        android:id="@+id/txt_stockiest1"
                        android:layout_width="@dimen/size13"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/mrg4"
                        android:textColor="@color/colorPrimary"
                        android:visibility="visible"
                        android:singleLine="true"
                        android:text="Stockiest (1)"
                        />



                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rr_testing_count_stockiest1"
                    android:layout_width="@dimen/size19"
                    android:layout_height="@dimen/size3"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    >


                    <RelativeLayout
                        android:id="@+id/rr_left_testing_stockiest1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/x_et_bg_cmpny_login"
                        android:elevation="@dimen/mrg1"
                        android:layout_toLeftOf="@+id/rr_stockiest1_qty"
                        android:layout_marginRight="@dimen/mrg1"

                        >

                        <EditText
                            android:id="@+id/spn_stockiest1"
                            android:layout_width="@dimen/size7"
                            android:layout_height="@dimen/size2"
                            android:text="Testing"
                            android:focusable="false"
                            android:singleLine="true"
                            android:paddingLeft="@dimen/size0"
                            android:textColor="@color/colorPrimary"
                            android:background="@android:color/transparent"
                            ></EditText>

                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rr_stockiest1_qty"
                        android:layout_width="@dimen/size4"
                        android:layout_height="@dimen/size2"
                        android:layout_alignParentRight="true"
                        android:background="@drawable/x_et_bg_cmpny_login"
                        android:elevation="@dimen/mrg1"
                        android:layout_marginRight="@dimen/mrg1"

                        >

                        <EditText
                            android:id="@+id/edt_stockiest1_qty"
                            android:layout_width="@dimen/size4"
                            android:layout_height="@dimen/size2"
                            android:focusable="false"
                            android:singleLine="true"
                            android:text="10"
                            android:layout_centerInParent="true"
                            android:paddingLeft="@dimen/size1"
                            android:textColor="@color/colorPrimary"
                            android:textColorHint="@color/colorPrimary"
                            android:textSize="@dimen/mrg4"
                            android:background="@android:color/transparent"
                            ></EditText>

                    </RelativeLayout>




                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rr_stockiest2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/size3"

                android:layout_below="@+id/rr_stockiest1"
                >

                <RelativeLayout
                    android:id="@+id/rr_stockiest2_full"
                    android:layout_marginTop="@dimen/mrg1"
                    android:layout_width="@dimen/size14"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/mrg2"
                    android:layout_centerVertical="true"

                    >
                    <TextView
                        android:id="@+id/txt_stockiest2"
                        android:layout_width="@dimen/size13"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/mrg4"
                        android:textColor="@color/colorPrimary"
                        android:visibility="visible"
                        android:singleLine="true"
                        android:text="Stockiest (2)"
                        />



                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rr_testing_count_stockiest2"
                    android:layout_width="@dimen/size19"
                    android:layout_height="@dimen/size3"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    >


                    <RelativeLayout
                        android:id="@+id/rr_left_testing_stockiest2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/x_et_bg_cmpny_login"
                        android:elevation="@dimen/mrg1"
                        android:layout_toLeftOf="@+id/rr_stockiest2_qty"
                        android:layout_marginRight="@dimen/mrg1"

                        >

                        <EditText
                            android:id="@+id/spn_stockiest2"
                            android:layout_width="@dimen/size7"
                            android:layout_height="@dimen/size2"
                            android:text="Testing"
                            android:focusable="false"
                            android:singleLine="true"
                            android:paddingLeft="@dimen/size0"
                            android:textColor="@color/colorPrimary"
                            android:background="@android:color/transparent"
                            ></EditText>

                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rr_stockiest2_qty"
                        android:layout_width="@dimen/size4"
                        android:layout_height="@dimen/size2"
                        android:layout_alignParentRight="true"
                        android:background="@drawable/x_et_bg_cmpny_login"
                        android:elevation="@dimen/mrg1"
                        android:layout_marginRight="@dimen/mrg1"

                        >

                        <EditText
                            android:id="@+id/edt_stockiest2_qty"
                            android:layout_width="@dimen/size4"
                            android:layout_height="@dimen/size2"
                            android:focusable="false"
                            android:singleLine="true"
                            android:text="10"
                            android:layout_centerInParent="true"
                            android:paddingLeft="@dimen/size1"
                            android:textColor="@color/colorPrimary"
                            android:textColorHint="@color/colorPrimary"
                            android:textSize="@dimen/mrg4"
                            android:background="@android:color/transparent"
                            ></EditText>

                    </RelativeLayout>




                </RelativeLayout>

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rr_stockiest3"
                android:layout_width="match_parent"
                android:layout_height="@dimen/size3"

                android:layout_below="@+id/rr_stockiest2"
                >

                <RelativeLayout
                    android:id="@+id/rr_stockiest3_full"
                    android:layout_marginTop="@dimen/mrg1"
                    android:layout_width="@dimen/size14"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/mrg2"
                    android:layout_centerVertical="true"

                    >
                    <TextView
                        android:id="@+id/txt_stockiest3"
                        android:layout_width="@dimen/size13"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/mrg4"
                        android:textColor="@color/colorPrimary"
                        android:visibility="visible"
                        android:singleLine="true"
                        android:text="Stockiest (3)"
                        />



                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rr_testing_count_stockiest3"
                    android:layout_width="@dimen/size19"
                    android:layout_height="@dimen/size3"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    >


                    <RelativeLayout
                        android:id="@+id/rr_left_testing_stockiest3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/x_et_bg_cmpny_login"
                        android:elevation="@dimen/mrg1"
                        android:layout_toLeftOf="@+id/rr_stockiest3_qty"
                        android:layout_marginRight="@dimen/mrg1"

                        >

                        <EditText
                            android:id="@+id/spn_stockiest3"
                            android:layout_width="@dimen/size7"
                            android:layout_height="@dimen/size2"
                            android:text="Testing"
                            android:focusable="false"
                            android:singleLine="true"
                            android:paddingLeft="@dimen/size0"
                            android:textColor="@color/colorPrimary"
                            android:background="@android:color/transparent"
                            ></EditText>

                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rr_stockiest3_qty"
                        android:layout_width="@dimen/size4"
                        android:layout_height="@dimen/size2"
                        android:layout_alignParentRight="true"
                        android:background="@drawable/x_et_bg_cmpny_login"
                        android:elevation="@dimen/mrg1"
                        android:layout_marginRight="@dimen/mrg1"

                        >

                        <EditText
                            android:id="@+id/edt_stockiest3_qty"
                            android:layout_width="@dimen/size4"
                            android:layout_height="@dimen/size2"
                            android:focusable="false"
                            android:singleLine="true"
                            android:text="10"
                            android:layout_centerInParent="true"
                            android:paddingLeft="@dimen/size1"
                            android:textColor="@color/colorPrimary"
                            android:textColorHint="@color/colorPrimary"
                            android:textSize="@dimen/mrg4"
                            android:background="@android:color/transparent"
                            ></EditText>

                    </RelativeLayout>




                </RelativeLayout>

            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rr_addchmist_cllcmplt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rr_stockiest3"
                android:layout_marginTop="@dimen/mrg2"
                android:layout_marginLeft="@dimen/mrg2"
                android:layout_marginRight="@dimen/mrg2"
                android:layout_marginBottom="@dimen/size0"



                ></RelativeLayout>




            </RelativeLayout><!--2222-->

    </RelativeLayout>


</RelativeLayout>